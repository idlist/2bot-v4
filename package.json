{
  "name": "2bot-v4",
  "version": "0.0.1",
  "private": true,
  "description": "A bot using Koishi v4",
  "author": "i'DLisT <me@idl.ist> (https://idl.ist)",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idlist/2bot-v4.git"
  },
  "bugs": {
    "url": "https://github.com/idlist/2bot-v4/issues"
  },
  "homepage": "https://github.com/idlist/2bot-v4#readme",
  "main": "index.js",
  "scripts": {
    "update": "git pull && git submodule update --init",
    "start": "koishi start",
    "test": "koishi start koishi.config.test.js",
    "build:canvas": "npm run build -w packages/canvas",
    "prod": "concurrently 'npm:build:*' && npm start",
    "reindex": "node scripts/reindex-teach.js",
    "count": "cd .. && cloc 2bot-v4 --exclude-dir=node_modules,test --include-ext=js,ts"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "js-yaml": "^4.0.0",
    "lodash": "^4.0.0",
    "luxon": "^2.0.0",
    "mysql": "^2.0.0",
    "node-schedule": "^2.0.0",
    "outdent": "^0.8.0",
    "simplify-chinese": "^1.0.0"
  },
  "devDependencies": {
    "@koishijs/cli": "^4.0.0",
    "@koishijs/plugin-adapter-discord": "^2.0.0",
    "@koishijs/plugin-adapter-onebot": "^4.0.0",
    "@koishijs/plugin-commands": "^1.2.0",
    "@koishijs/plugin-console": "^3.0.0",
    "@koishijs/plugin-database-mysql": "^4.0.0",
    "@koishijs/plugin-manager": "^1.0.0",
