    "concurrently": "^7.0.0",
    "eslint": "^8.0.0",
    "koishi": "^4.0.0",
    "koishi-plugin-assets-smms": "^1.0.0-rc.1"
  },
  "workspaces": [
    "packages/*"
  ]
